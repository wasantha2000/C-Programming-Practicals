#include<stdio.h>

int main(){

int num,i;
int sum=0;

printf("Input size of the array:");
scanf("%d",&num);

int arr[num];
printf("Input elements to array:\n");
for(i=0; i<num; i++){
	printf("arr[%d]=",i);
	scanf("%d",&arr[i]);
}

for(i=0; i<num; i++){
	sum+=arr[i];
}
printf("\nSum of elements=%d",sum);

return 0;
}

