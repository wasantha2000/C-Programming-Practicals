#include<stdio.h>

int main(){

int num1,num2;

printf("Input first number:");
scanf("%d",&num1);
printf("Input second number:");
scanf("%d",&num2);

if(num1<num2){
	printf("Maximum number is %d",num2);
}
else{
	printf("Maximum number is %d",num1);
}


return 0;
}

